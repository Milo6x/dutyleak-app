{
  "name": "dutyleak-improved",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\"__tests__\" --testPathIgnorePatterns=\"e2e\"",
    "test:integration": "jest --testPathPattern=\"api.*test\" --testTimeout=10000",
    "test:performance": "jest --testPathPattern=\"performance.test\" --testTimeout=30000",
    "test:security": "jest --testPathPattern=\"security.test\"",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:all": "./scripts/run-tests.sh",
    "type-check": "tsc --noEmit",
    "setup-notifications": "node scripts/setup-notifications.js",
    "notifications:setup": "node scripts/setup-notifications.js setup",
    "notifications:verify": "node scripts/setup-notifications.js verify",
    "notifications:sample": "node scripts/setup-notifications.js sample",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "db:reset": "supabase db reset",
    "docker:build": "docker build -t dutyleak .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local dutyleak",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/auth-helpers-nextjs": "^0.8.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^2.30.0",
    "framework7-react": "^8.3.0",
    "lucide-react": "^0.511.0",
    "next": "14.0.3",
    "openai": "^5.1.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.21.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.12.0",
    "react-resizable-panels": "^3.0.2",
    "react-table": "^7.8.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "swr": "^2.2.4",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.38",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-dom": "^18.2.17",
    "@types/react-table": "^7.7.18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}
